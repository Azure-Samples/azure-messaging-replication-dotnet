{
    "configurationSource": "config",
    "bindings" : [
        {
            "direction": "in",
            "type": "serviceBusTrigger",
            "connection": "QueueAToQueueB-source-connection",
            "queueName": "queue-a",
            "name": "input" 
        },
        {
            "direction": "out",
            "type": "serviceBus",
            "connection": "QueueAToQueueB-target-connection",
            "queueName": "queue-b",
            "name": "output"
        }
    ],
    "retry": {
        "strategy": "exponentialBackoff",
        "maxRetryCount": -1,
        "minimumInterval": "00:00:05",
        "maximumInterval": "00:05:00"
    },
    "disabled": false,
    "scriptFile": "../bin/Azure.Messaging.Replication.dll",
    "entryPoint": "Azure.Messaging.Replication.ServiceBusReplicationTasks.ForwardToServiceBus"
}